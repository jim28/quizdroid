<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wyliao.edu.washington.quizdriod" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:name=".QuizApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".NextActivity">
        </activity>
        
        <activity android:name=".UserSettingActivity">
            </activity>
        <receiver android:name=".AlarmReceiver">
            <intent-filter>
                <action android:name="wyliao.edu.washington.quizdroid">
                </action>
            </intent-filter>
        </receiver>

        <service
            android:enabled="true"
            android:name="wyliao.edu.washington.quizdriod.DownloadService"
            >

            </service>

    </application>

</manifest>
